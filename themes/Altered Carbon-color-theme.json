{
  "name": "Altered Carbon",
  "colors": {
    "editor.background": "#172030",
    "editor.foreground": "#bac8cb",
    "editorLineNumber.foreground": "#657778",
    "editorCursor.foreground": "#FFCB6B",
    "editor.selectionBackground": "#0A677A",
    "editor.inactiveSelectionBackground": "#1E2A3E",
    "editor.lineHighlightBackground": "#1E2A3E",
    "editorWhitespace.foreground": "#213047",
    "editorIndentGuide.background1": "#213047",
    "editorIndentGuide.activeBackground1": "#586e75",
    "editorBracketMatch.border": "#2aa198",
    "editorBracketMatch.background": "#1E2A3E",
    "editorGroupHeader.tabsBackground": "#1E2A3E",
    "sideBarTitle.foreground": "#2aa198",
    "sideBar.background": "#1E2A3E",
    "sideBar.foreground": "#bac8cb",
    "activityBar.background": "#172030",
    "activityBar.foreground": "#bac8cb",
    "statusBar.background": "#213047",
    "statusBar.noFolderBackground": "#213047",
    "statusBar.debuggingBackground": "#FFCB6B",
    "titleBar.activeBackground": "#172030",
    "titleBar.activeForeground": "#bac8cb",
    "tab.activeBackground": "#213047",
    "tab.inactiveBackground": "#1A2434",
    "tab.activeForeground": "#bac8cb",
    "tab.inactiveForeground": "#586e75",
    "tab.border": "#172030",
    "panel.background": "#1E2A3E",
    "panel.border": "#213047",
    "dropdown.background": "#213047",
    "dropdown.foreground": "#ffffff",
    "dropdown.border": "#3c3c3c",
    "dropdown.listBackground": "#252526",
    "menu.background": "#213047",
    "menu.foreground": "#ffffff",
    "menu.selectionBackground": "#2aa198",
    "menu.selectionForeground": "#ffffff",
    "menu.border": "#3c3c3c"
  },
  "tokenColors": [
    {
      "name": "Comentarios",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#657778",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Palabras clave y tipos",
      "scope": ["keyword", "storage.type"],
      "settings": {
        "foreground": "#0c9294",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "identifier", "meta.definition.variable.name"],
      "settings": {
        "foreground": "#bac8cb"
      }
    },
    {
      "name": "Cadenas de texto",
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#DDEB9D"
      }
    },
    {
      "name": "Números y booleanos",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#8E7DBE"
      }
    },
    {
      "name": "Funciones",
      "scope": ["entity.name.function", "meta.function-call"],
      "settings": {
        "foreground": "#44a6d2"
      }
    },
    {
      "name": "Tipos y clases",
      "scope": [
        "entity.name.type",
        "support.type",
        "meta.class",
        "entity.name.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#e1e797"
      }
    },
    {
      "name": "Modificadores y operadores",
      "scope": ["storage.modifier", "keyword.operator"],
      "settings": {
        "foreground": "#af304a"
      }
    },

    {
      "name": "Etiquetas HTML",
      "scope": ["meta.tag", "entity.name.tag"],
      "settings": {
        "foreground": "#F92672"
      }
    },
    {
      "name": "Atributos HTML",
      "scope": ["entity.other.attribute-name", "attribute.name"],
      "settings": {
        "foreground": "#02858e"
      }
    },
    {
      "name": "Texto y estructura HTML",
      "scope": [
        "source.html",
        "text.html.basic",
        "meta.embedded.block.html",
        "meta.tag.inline.any.html",
        "meta.tag.block.any.html",
        "meta.tag.structure.any.html"
      ],
      "settings": {
        "foreground": "#abb9bc"
      }
    },
    {
      "name": "Entidades HTML",
      "scope": ["constant.character.entity", "punctuation.definition.entity"],
      "settings": {
        "foreground": "#e0555e"
      }
    },
    {
      "name": "Delimitadores de etiquetas HTML",
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#586e75"
      }
    },
    {
      "name": "Clases HTML",
      "scope": [
        "meta.attribute.class.html string.quoted.double.html",
        "meta.attribute.class.html string.quoted.single.html"
      ],
      "settings": {
        "foreground": "#DDEB9D"
      }
    },

    {
      "name": "Selectores CSS",
      "scope": [
        "meta.selector",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#027889"
      }
    },
    {
      "name": "Valores CSS",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#44a6d2"
      }
    },
    {
      "name": "Pseudo-clases y elementos CSS",
      "scope": [
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css"
      ],
      "settings": {
        "foreground": "#f0c674",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Propiedades CSS",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#abb9bc"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": ["entity.name.tag.css"],
      "settings": {
        "foreground": "#e0555e"
      }
    },

    {
      "name": "Palabras clave de control JS",
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#f0c674",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Declaraciones de variables JS/TS",
      "scope": [
        "storage.type.variable.js",
        "storage.type.variable.ts",
        "variable.language"
      ],
      "settings": {
        "foreground": "#f3b2ab"
      }
    },
    {
      "name": "Declaración de funciones JS",
      "scope": ["storage.type.function", "keyword.control.function"],
      "settings": {
        "foreground": "#44a6d2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Agrupadores y llaves",
      "scope": [
        "punctuation.section.block.begin",
        "punctuation.section.block.end",
        "punctuation.section.group.begin",
        "punctuation.section.group.end",
        "punctuation.section.brackets.begin",
        "punctuation.section.brackets.end",
        "punctuation.bracket"
      ],
      "settings": {
        "foreground": "#e1e797"
      }
    },

    {
      "name": "Cadenas en JSON",
      "scope": ["source.json string.quoted.double.json"],
      "settings": {
        "foreground": "#DDEB9D"
      }
    },
    {
      "name": "Claves en JSON",
      "scope": ["source.json support.type.property-name.json"],
      "settings": {
        "foreground": "#abb9bc"
      }
    },
    {
      "name": "Constantes en JSON",
      "scope": [
        "source.json constant.numeric.json",
        "source.json constant.language.boolean.json",
        "source.json constant.language.null.json"
      ],
      "settings": {
        "foreground": "#ff6188"
      }
    },

    {
      "name": "Errores y sintaxis inválida",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ff4262",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown - Plain",
      "scope": [
        "text.html.markdown",
        "punctuation.definition.list_item.markdown"
      ],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline",
      "scope": ["text.html.markdown markup.inline.raw.markdown"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline Punctuation",
      "scope": [
        "text.html.markdown markup.inline.raw.markdown punctuation.definition.raw.markdown"
      ],
      "settings": {
        "foreground": "#65737E"
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": [
        "markdown.heading",
        "markup.heading | markup.heading entity.name",
        "markup.heading.markdown punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#F92672"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold", "markup.bold string"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#F92672"
      }
    },
    {
      "name": "Markup - Bold-Italic",
      "scope": [
        "markup.bold markup.italic",
        "markup.italic markup.bold",
        "markup.quote markup.bold",
        "markup.bold markup.italic string",
        "markup.italic markup.bold string",
        "markup.quote markup.bold string"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#F92672"
      }
    },
    {
      "name": "Markup - Underline",
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "Markdown - Blockquote",
      "scope": ["markup.quote punctuation.definition.blockquote.markdown"],
      "settings": {
        "foreground": "#65737E"
      }
    },
    {
      "name": "Markup - Quote",
      "scope": ["markup.quote"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown - Link",
      "scope": ["string.other.link.title.markdown"],
      "settings": {
        "foreground": "#14cbce"
      }
    },
    {
      "name": "Markdown - Link Description",
      "scope": ["string.other.link.description.title.markdown"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Markdown - Link Anchor",
      "scope": ["constant.other.reference.link.markdown"],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "Markup - Raw Block",
      "scope": ["markup.raw.block"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Markdown - Raw Block Fenced",
      "scope": ["markup.raw.block.fenced.markdown"],
      "settings": {
        "foreground": "#00000050"
      }
    },
    {
      "name": "Markdown - Fenced Bode Block",
      "scope": ["punctuation.definition.fenced.markdown"],
      "settings": {
        "foreground": "#00000050"
      }
    },
    {
      "name": "Markdown - Fenced Bode Block Variable",
      "scope": [
        "markup.raw.block.fenced.markdown",
        "variable.language.fenced.markdown",
        "punctuation.section.class.end"
      ],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "Markdown - Fenced Language",
      "scope": ["variable.language.fenced.markdown"],
      "settings": {
        "foreground": "#65737E"
      }
    },
    {
      "name": "Markdown - Separator",
      "scope": ["meta.separator"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#65737E"
      }
    },
    {
      "name": "Markup - Table",
      "scope": ["markup.table"],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    }
  ]
}
